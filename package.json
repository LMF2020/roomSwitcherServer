{
  "name": "pair-switcher",
  "version": "1.0.1",
  "main": "dist/main/index.cjs",
  "description": "",
  "author": "Jed",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:x64": "npm run typecheck && electron-vite build --config electron.x64.vite.config.ts",
    "build:mac:x64": "npm run build:x64 && electron-builder --mac --config electron-builder-x64.json",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.3.2",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/ip": "^1.1.3",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.19",
    "electron": "latest",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-builder": "^0.1.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-log": "^5.1.5",
    "electron-settings": "^4.0.4",
    "electron-store": "8.1.0",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.7.3",
    "ip": "^2.0.1",
    "node-machine-id": "^1.1.12",
    "sass": "^1.77.1",
    "socket.io": "^4.7.5",
    "sudo-prompt": "^9.2.1",
    "uuid": "^9.0.1",
    "vue-router": "^4.3.2",
    "vuex": "next"
  }
}
